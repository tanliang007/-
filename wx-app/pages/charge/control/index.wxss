/* pages/charging-controls/index.wxss */

page {
	padding-bottom: 20rpx;
}

.bg-top {
  padding: 50rpx 0 30rpx 0;
  background: #1ec678;
}

.redbg-top {
  padding: 50rpx 0 30rpx 0;
  background: #eb5150;
}

.outer {
  height: 360rpx;
  width: 360rpx;
  position: relative;
  margin: 0rpx auto 0;
}

.content-Wrapper {
  position: absolute;
  width: 100%;
  top: 50%;
  transform: translateY(-50%);
  text-align: center;
  color: #fff;
}

.countdown {
  font-size: 38rpx;
  font-weight: bold;
  line-height: 38rpx;
  margin-top: -30rpx;
}

.countDesc {
  font-size: 28rpx;
  line-height: 38rpx;
}

.chargearea {
  text-align: center;
  margin-top: 25rpx;
}

.icon {
  width: 60rpx;
  height: 60rpx;
  display: inline-block;
  vertical-align: middle;
}

.areaDtl {
  display: inline-block;
  vertical-align: middle;
  color: #fff;
  font-size: 32rpx;
  margin-top: 22rpx;
  margin-bottom: 12rpx;
}

.changeicon {
  font-size: 50rpx;
  line-height: 50rpx;
  color: #fff;
}

.portlocation {
  color: #fff;
  font-size: 24rpx;
  padding: 0 32rpx;
}

.content {
  background-image: url('http://charge-pile.oss-cn-hangzhou.aliyuncs.com/icon/chargecloud.png');
  background-repeat: none;
  background-size: 100% 100%;
  width: 260rpx;
  height: 260rpx;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  top: 50%;
  z-index: 3;
  transform: translate(-50%, -50%);
}

.redcontent {
  background-image: url('http://charge-pile.oss-cn-hangzhou.aliyuncs.com/icon/nochargecloud.png');
  background-repeat: none;
  background-size: 100% 100%;
  width: 260rpx;
  height: 260rpx;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  top: 50%;
  z-index: 3;
  transform: translate(-50%, -50%);
}

.box {
  position: absolute;
  width: 300rpx;
  height: 300rpx;
  left: 50%;
  top: 50%;
  z-index: 4;
  background-image: url('data:image/png;base64,');
  background-size: 280rpx 280rpx;
  background-position: center;
  background-repeat: no-repeat;
  animation: moveover 3s cubic-bezier(0.42, 0, 0.58, 1) infinite;
  transform-origin: 50% 50%;
}

.boxNoAnimation {
  position: absolute;
  width: 300rpx;
  height: 300rpx;
  left: 50%;
  top: 50%;
  z-index: 4;
  transform: translate(-50%, -50%);
  background-image: url('data:image/png;base64,');
  background-size: 280rpx 280rpx;
  background-position: center;
  background-repeat: no-repeat;
}

.box1 {
  position: absolute;
  width: 140rpx;
  height: 280rpx;
  border-radius: 140rpx 0 0 140rpx;
  background: linear-gradient(#cad38c, #d0cfcf);
  z-index: 2;
}

.box2 {
  position: absolute;
  width: 140rpx;
  height: 280rpx;
  border-radius: 0 140rpx 140rpx 0;
  left: 50%;
  background: linear-gradient(#eee, #d0cfcf);
  z-index: 1;
}

.line {
  background-image: url('http://charge-pile.oss-cn-hangzhou.aliyuncs.com/icon/whiteaperture.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  width: 330rpx;
  height: 330rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  animation: bigCircleAnimation 2s ease-in-out infinite alternate;
}

.lineNoAnimation {
  background-image: url('http://charge-pile.oss-cn-hangzhou.aliyuncs.com/icon/whiteaperture.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  width: 330rpx;
  height: 330rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.hehongColor {
  color: #c30;
}

.defaultColor {
  color: #fff;
}

/*旋转动画*/

@keyframes moveover {
  0% {
    transform: translate(-50%, -50%) rotate(0deg);
  }

  100% {
    transform: translate(-50%, -50%) rotate(360deg);
  }
}

@keyframes bigCircleAnimation {
  0% {
    -webkit-filter: opacity(10%);
    filter: opacity(10%);
  }

  100% {
    -webkit-filter: opacity(100%);
    filter: opacity(100%);
  }
}

@keyframes bigCircleAnimation {
  0% {
    -webkit-filter: opacity(10%);
    filter: opacity(10%);
  }

  100% {
    -webkit-filter: opacity(100%);
    filter: opacity(100%);
  }
}

.bg-cloud {
  background: url('http://charge-pile.oss-cn-hangzhou.aliyuncs.com/icon/chargecloud2.png') no-repeat;
  height: 150rpx;
  background-size: contain;
  background-repeat: none;
}

.bg-redcloud {
  background: url('http://charge-pile.oss-cn-hangzhou.aliyuncs.com/icon/nochargecloud2.png') no-repeat;
  height: 150rpx;
  background-size: contain;
  background-repeat: none;
}

.shuntiming {
  width: 100%;
  text-align: center;
}

.starttiming {
  float: left;
  width: 50%;
}

.monetary {
  float: left;
  width: 50%;
}

.starttxt {
  color: #333;
  font-size: 30rpx;
}

.starttime {
  font-weight: bold;
  font-size: 36rpx;
}

.monetarytxt {
  color: #333;
  font-size: 30rpx;
}

.monetarynum {
  color: #e97841;
  font-size: 36rpx;
  font-weight: bold;
}

/*结束充电*/

.greenbutton {
  width: 300rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: linear-gradient(to right, #1ec678, #3ace57);
  font-size: 36rpx;
  text-align: center;
  border-radius: 50rpx;
  color: #fff;
  margin: 120rpx auto 0;
}

/*去除button默认边框*/

.greenbutton::after {
  border: none;
  border-radius: 0;
}

.redbutton {
  width: 300rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #eb5150;
  font-size: 36rpx;
  text-align: center;
  border-radius: 50rpx;
  color: #fff;
  margin: 120rpx auto 0;
}

.redbutton::after {
  border: none;
  border-radius: 0;
}

.greenbutton-hover {
  width: 300rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: linear-gradient(to right, #1ec678, #3ace57);
  font-size: 36rpx;
  text-align: center;
  border-radius: 50rpx;
  color: #fff;
  margin: 120rpx auto 0;
  position: relative;
  top: 1rpx;
}

.redbutton-hover {
  width: 300rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #eb5150;
  font-size: 36rpx;
  text-align: center;
  border-radius: 50rpx;
  color: #fff;
  margin: 120rpx auto 0;
  position: relative;
  top: 1rpx;
}

/*自定义模态框样式*/

/*button*/

.btn {
  width: 80%;
  padding: 20rpx 0;
  border-radius: 10rpx;
  text-align: center;
  margin: 40rpx 10%;
  background: #000;
  color: #fff;
}

/*mask*/

.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}

/*content*/

.drawer_box {
  width: 600rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 1001;
  background: #fafafa;
  transform: translate(-50%, -60%);
  border-radius: 3px;
}

.drawer_title {
  padding: 15px;
  font: 35rpx "microsoft yahei";
  text-align: center;
}

.drawer_content {
  padding: 40rpx 0 100rpx 50rpx;
}

.btn_ok {
  padding: 30rpx 50rpx 30rpx 0rpx;
  font: 40rpx "microsoft yahei";
  /* float: right; */
  text-align: right;
  /* border-top: 1px solid #e8e8ea; */
  color: #3cc51f;
}

.col-0 {
  line-height: 100rpx;
  text-align: center;
}

/* 断电圆圈中的样式 */

.suspendimg {
  width: 101rpx;
  height: 86rpx;
  display: block;
  margin: -60rpx auto 10rpx;
}

.suspendimg image {
  width: 100%;
  height: 100%;
}

.suspendtext {
  font-size: 30rpx;
}

/*  底部断电文字提示样式 */

.warningtip {
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  width: 85%;
  margin: 120rpx auto;
}

.warningtip .warningtipimg image {
  width: 94rpx;
  height: 80rpx;
  display: block;
}

.warningtip .warningtipimg {
  display: flex;
}

.warningtip .warningtiptxt {
  display: inline-block;
  font-size: 28rpx;
  color: #ed6263;
  margin-left: 20rpx;
}

/* 断电的时候按钮充电样式  */

.powerToStop {
  margin: 20rpx auto;
  box-sizing: border-box;
  border: 1rpx solid #c9c9c9;
  width: 300rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #eb5150;
  font-size: 36rpx;
  text-align: center;
  border-radius: 50rpx;
  color: #fff;
}

/* 首页返回样式 */

.backindexbtn {
  margin: 50rpx auto;
  box-sizing: border-box;
  border: 1rpx solid #c9c9c9;
  width: 300rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  font-size: 36rpx;
  text-align: center;
  border-radius: 50rpx;
  color: #767676;
}

/* 查询充电  */

.searchcable {
  width: 600rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 1001;
  background: #fafafa;
  transform: translate(-50%, -60%);
  border-radius: 3px;
}

.searchcable-b {
  padding: 40rpx 0 70rpx 50rpx;
}

/* 自定义模态框的文字大小 */

.zidongfinish, .searchcable-m {
  font-size: 40rpx;
}
