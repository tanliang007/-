/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background: yellowgreen;
}

.clearfloat::before, .clearfloat::after {
  content: "";
  display: block;
  clear: both;
  line-height: 0;
  visibility: hidden;
  height: 0;
}

.clearfloat {
  zoom: 1;
}
button::after {
  border: none;
  border-radius: 0;
}
/** 颜色 **/

.text-white {
  color: white;
}

.text-primary {
  color: #1aad19;
}

